{
  "manifest_version": 3,
  "name": "LinkedIn AI Reply",
  "version": "1.0",
  "description": "Assists in generating replies to LinkedIn messages.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "action": {
    "default_popup": "index.html"
  }
}
